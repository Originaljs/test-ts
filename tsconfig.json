{
  "compilerOptions": {
    "outDir": "./",
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules","**/*.spec.ts"]
}
